{
    "name": "vega/cms",
    "description": "CMS for developers",
    "keywords": ["laravel", "cms"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Stoyan Todorov",
            "email": "stoyan_11@abv.bg",
            "role": "Original idea, Developer"
        },
        {
            "name": "Nedyalko Raykov",
            "email": "ned.raykov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "fideloper/proxy": "^4.2",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "spatie/laravel-translatable": "^4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "ext-json": "*",
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "facade/ignition": "^2.0"
    },
    "autoload": {
        "classmap": [
            "src/database/seeds",
            "src/database/factories"
        ],
        "psr-4": {
            "App\\": "src",
            "Vega\\Cms\\Src\\": "packages/vega/cms/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vega\\Cms\\Src\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vega\\Cms\\Src\\App\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
